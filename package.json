{
  "name": "big-config",
  "version": "3.2.0",
  "description": "Easily manage configuration settings for small to very large projects",
  "repository": "natesilva/big-config",
  "license": "BSD-3-Clause",
  "author": "Nate Silva <nate@natesilva.com>",
  "keywords": [
    "conf",
    "config",
    "configuration",
    "env",
    "environment",
    "json",
    "yaml"
  ],
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "scripts": {
    "clean": "del-cli build",
    "build": "tsc",
    "prettier": "prettier --write \"{src,test}/**/*.?(ts|json)\"",
    "test": "mocha",
    "test:watch": "mocha -w",
    "coverage": "nyc --reporter lcov --reporter text --extension .ts mocha",
    "prepublishOnly": "tsc",
    "postpublish": "del-cli build"
  },
  "engines": {
    "node": ">=10.10.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^3.12.4",
    "@types/json5": "0.0.30",
    "@types/lodash": "^4.14.154",
    "@types/mocha": "^7.0.2",
    "@types/node": "^12.12.43",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "del-cli": "^3.0.1",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "mocha": "^7.2.0",
    "np": "^6.2.3",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "testdouble": "^3.16.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "js-yaml": "^3.14.0",
    "json5": "^2.1.3",
    "lodash": "^4.17.15",
    "type-fest": "^0.15.0"
  }
}
